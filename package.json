{
  "name": "gpt-token-counter",
  "displayName": "GPT Token Counter",
  "description": "Count OpenAI GPT tokens in a document or selected text",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.countTokens",
        "title": "Count Tokens"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/mocha": "^8.2.0",
    "@types/vscode": "^1.56.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "mocha": "^8.4.0"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.0"
  }
}
